{
  "name": "betLectio",
  "action": {},
  "manifest_version": 2,
  "version": "0.1",
  "description": "Smart design for lectio",
  "content_scripts": [
    {
      "matches": [ "https://www.lectio.dk/*" ],
      "css": [ "style.css" ]
    },
    {
      "matches": [ "https://www.lectio.dk/*" ],
      "js": [ "script.js" ]
    }
  ],
  "permissions": [ "https://www.lectio.dk/*", "storage" ],
  "web_accessible_resources": [ "*" ]
}
